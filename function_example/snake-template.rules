rule EXPONENTIAL:
    input:
        input_file="cars.csv"
    output:
        output_file="cars.hdf5"
    run:
       from my_functions import exponential
       exponential(input.input_file, output.output_file)


rule EXPONENTIAL2:
    input:
        input_file="cars.csv"
    output:
        output_file="cars2.hdf5"
    run:
       from my_functions import exponential
       exponential(input.input_file, output.output_file, degree=1)


rule CREATE_MODELS:
    input:
        input_files=["cars.hdf5", "cars2.hdf5"]
    output:
        output_file="models.hdf5"
    run:
       from my_functions import create_models
       create_models(input.input_files, output.output_file)

